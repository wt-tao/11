/* pages/uesr/uesr.wxss */
page{background-color: #F1F1F1}
.user{width: 100%;height: 300rpx;background-color: #F67B0E}
.user_img{width:150rpx;height:150rpx;border-radius: 50%;border: 1rpx solid black;margin-left:20rpx;margin-top: 30rpx;}
.user_info{width: 50%;margin-left: 20rpx;margin-top:30rpx;}
.info_name{width: 100%;font-size: 34rpx;color: #FFFBF9;line-height: 100rpx;}
.my_jifen{width: 200rpx;line-height:50rpx;font-size: 22rpx;border-radius: 30rpx; color: #FDC6AB;border: 1rpx solid #FDC6AB;text-align: center}
.end{width: 120rpx;text-align: center;line-height: 60rpx;border-radius: 5rpx;color: #FFFBF6;border: 1rpx solid #FFFBF6;margin-top:40rpx;height:60rpx;font-size: 28rpx;margin-left:30rpx;}

.white{width: 96%;margin-left: 2%;background-color: white;border-radius: 15rpx;height:200rpx;margin-top:-80rpx;}
.white_list{width: 50%;height:180rpx;border-right: 1rpx solid #EEEEEE;margin-top:10rpx;}
.white_number{width: 100%;text-align: center;color: #E31D1D;font-size: 60rpx;margin-top: 20rpx;}
.white_but{width: 100%;height: 60rpx;line-height: 60rpx;font-size: 28rpx;color: #777}

.list{width: 100%;height: 100rpx;line-height: 100rpx;font-size: 30rpx;color: #4A4A4A;background-color: white;border-bottom: 1rpx solid #F3F3F3}
.list_left{width: 90%;}