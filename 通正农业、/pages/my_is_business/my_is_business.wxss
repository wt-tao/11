page{background-color: #f1f1f1}
.head{position: relative;border: 1rpx solid black;background-color: white}
.slide-image{ width: 100%;}
.swiper-item{height:400rpx;display: block;}
.menu-swiper-item{height:400rpx;}

.white{width: 96%;margin-left: 2%;background-color: white;border-radius: 15rpx;height:200rpx;margin-top:-80rpx;position: relative}
.white_list{width: 50%;height:180rpx;border-right: 1rpx solid #EEEEEE;margin-top:10rpx;}
.white_number{width: 100%;text-align: center;color: #E31D1D;font-size: 60rpx;margin-top: 20rpx;}
.white_but{width: 100%;height: 60rpx;line-height: 60rpx;font-size: 28rpx;color: #777}

.div{background-color: white;flex-wrap: wrap;margin-top:20rpx;}
.div_list{border-right: 1rpx solid #ECECEC;border-bottom: 1rpx solid #ECECEC;width: 33%;height:220rpx;}
.div_text{width: 100%;text-align: center;font-size: 30rpx;color: #555}