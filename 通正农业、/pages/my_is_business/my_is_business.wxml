<!--pages/my_is_business/my_is_business.wxml-->
<view class='head'>
     <swiper indicator-dots="true" autoplay="true" interval="3000" indicator-color='white' indicator-active-color='#FF7F01' duration="1000" class="swiper-item" circular="true">
          <block wx:for="{{banner}}" wx:for-index="index">
              <swiper-item>
                <image src="{{item.img}}" id='{{item.id}}' class="slide-image" style='width:100%;height:100%' mode="aspectFill" bindtap='nack' />
              </swiper-item>
          </block>
     </swiper>
</view>

<view class='white flex'>
    <view class='white_list' bindtap='my_income'>
        <view class='white_number'>518.00</view>
        <view class='white_but flex'>
            <image src='../../img/tc-237 (1).png' style='width:30rpx;margin-top:15rpx;margin-left:100rpx;' mode='widthFix'/>
            <view style='margin-left:20rpx;'>我的收益</view>
        </view>
    </view>
    <view class='white_list' bindtap='fans'>
        <view class='white_number'>800</view>
        <view class='white_but flex'>
            <image src='../../img/tc-237 (2).png' style='width:30rpx;margin-top:15rpx;margin-left:100rpx;' mode='widthFix'/>
            <view style='margin-left:20rpx;'>我的粉丝</view>
        </view>
    </view>
</view>

<view class='div flex'>
    <view class='div_list' bindtap='commission_setting'>
        <image src='../../img/tc-257 (1).png' style='width:50%;margin-left:25%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text'>佣金设置</view>
    </view>
    <view class='div_list' bindtap='business_photo'>
        <image src='../../img/tc-257 (4).png' style='width:50%;margin-left:25%;margin-top:40rpx;' mode='widthFix'/>
        <view class='div_text' style='margin-top:10rpx;'>商家图片</view>
    </view>
    <view class='div_list' bindtap='business_introduce'>
        <image src='../../img/tc-257 (8).png' style='width:40%;margin-left:30%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text' style='margin-top:10rpx;'>商家介绍</view>
    </view>
    <view class='div_list'>
        <image src='../../img/tc-257 (2).png' style='width:45%;margin-left:27.5%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text'>我要提现</view>
    </view>
    <view class='div_list'>
        <image src='../../img/tc-257 (9).png' style='width:45%;margin-left:27.5%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text' >营业明细</view>
    </view>
    <view class='div_list'>
        <image src='../../img/tc-257 (6).png' style='width:50%;margin-left:25%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text' style='margin-top:5rpx;'>收到的评论</view>
    </view>
    <view class='div_list' bindtap='business_hours'>
        <image src='../../img/tc-257 (5).png' style='width:50%;margin-left:25%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text'>营业时间</view>
    </view>
    <view class='div_list' bindtap='information'>
        <image src='../../img/tc-257 (3).png' style='width:35%;margin-left:32.5%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text' style='margin-top:10rpx;'>联系方式</view>
    </view>
    <view class='div_list'>
        <image src='../../img/tc-257 (7).png' style='width:50%;margin-left:25%;margin-top:20rpx;' mode='widthFix'/>
        <view class='div_text'>我的收款码</view>
    </view>
  
</view>
